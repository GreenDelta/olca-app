	<extension
			point="org.eclipse.ui.navigator.viewer">
		<viewer
				viewerId="views.navigation">
			<options>
				<property
						name="org.eclipse.ui.navigator.hideCollapseAllAction"
						value="true">
				</property>
				<property
						name="org.eclipse.ui.navigator.hideAvailableCustomizationsDialog"
						value="true">
				</property>
			</options>
		</viewer>
		<viewerContentBinding
				viewerId="views.navigation">
			<includes>
				<contentExtension
						isRoot="true"
						pattern="navigatorcontent.context">
				</contentExtension>
				<contentExtension
						pattern="viewer.filter.emptycategory">
				</contentExtension>
				<contentExtension
						pattern="viewer.filter.productflow">
				</contentExtension>
				<contentExtension
						pattern="viewer.filter.elementaryflow">
				</contentExtension>
				<contentExtension
						pattern="viewer.filter.wasteflow">
				</contentExtension>
				<contentExtension
						pattern="navigator.linkHelper">
				</contentExtension>
			</includes>
		</viewerContentBinding>
		<viewerActionBinding
				viewerId="views.navigation">
			<includes>
				<actionExtension
						pattern="actionprovider.navigation">
				</actionExtension>
			</includes>
		</viewerActionBinding>
		<dragAssistant
				class="org.openlca.app.navigation.NavigationDragAssistant"
				viewerId="views.navigation">
		</dragAssistant>
	</extension>

	<extension
			point="org.eclipse.ui.navigator.navigatorContent">
		<navigatorContent
				activeByDefault="true"
				contentProvider="org.openlca.app.navigation.NavigationContentProvider"
				id="navigatorcontent.context"
				labelProvider="org.openlca.app.navigation.NavigationLabelProvider"
				name="%DataProvider"
				priority="normal">
			<triggerPoints>
				<instanceof
						value="org.openlca.app.navigation.elements.INavigationElement">
				</instanceof>
			</triggerPoints>
			<commonSorter
					class="org.openlca.app.navigation.NavigationComparator">
			</commonSorter>
			<dropAssistant
					class="org.openlca.app.navigation.NavigationDropAssistant"
					id="dropassistant.navigation">
				<possibleDropTargets>
					<instanceof
							value="org.openlca.app.navigation.elements.INavigationElement">
					</instanceof>
				</possibleDropTargets>
			</dropAssistant>
			<possibleChildren>
				<instanceof
						value="org.openlca.app.navigation.elements.INavigationElement">
				</instanceof>
			</possibleChildren>
		</navigatorContent>
		<actionProvider
				class="org.openlca.app.navigation.actions.NavigationMenu"
				id="actionprovider.navigation"
				priority="normal">
			<enablement/>
		</actionProvider>
		<commonFilter
				activeByDefault="false"
				class="org.openlca.app.navigation.filters.EmptyCategoryFilter"
				id="viewer.filter.emptycategory"
				name="%EmptyCategories">
		</commonFilter>
		<commonFilter
				activeByDefault="false"
				class="org.openlca.app.navigation.filters.ProductFlowFilter"
				id="viewer.filter.productflow"
				name="%ProductFlows">
		</commonFilter>
		<commonFilter
				activeByDefault="false"
				class="org.openlca.app.navigation.filters.ElementaryFlowFilter"
				id="viewer.filter.elementaryflow"
				name="%ElementaryFlows">
		</commonFilter>
		<commonFilter
				activeByDefault="false"
				class="org.openlca.app.navigation.filters.WasteFlowFilter"
				id="viewer.filter.wasteflow"
				name="%WasteFlows">
		</commonFilter>
	</extension>

